{
  "name": "kafu_hostels",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "seed": "npx ts-node lib/sanity/seed.ts",
    "seed:api": "curl -X GET \"http://localhost:8081/api/hostels/seed\"",
    "build:android:dev": "eas build --profile development:device --platform android",
    "build:android:preview": "eas build --profile preview:device --platform android",
    "build:android:prod": "eas build --profile production --platform android",
    "build:ios:dev": "eas build --profile development:device --platform ios",
    "build:ios:preview": "eas build --profile preview:device --platform ios",
    "export:web": "npx expo export --platform web",
    "build:web": "npx eas deploy"
  },
